{
  "name": "elm-instant",
  "main": "./lib/elm-instant",
  "version": "0.2.3",
  "description": "Try your elm code from the editor. Provides a visual REPL and a preview pane",
  "keywords": [
    "elm",
    "preview",
    "instant",
    "playground"
  ],
  "activationCommands": {
    "atom-workspace": "elm-instant:toggle"
  },
  "scripts": {
    "lint": "standard lib",
    "install-elm-packages": "cd lib/compilers/elm-compiler/temp && rm -rf elm-stuff && elm-package install -y && cd -",
    "preinstall": "npm run install-elm-packages"
  },
  "repository": "https://github.com/mukeshsoni/elm-instant",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-package-deps": "^4.3.1",
    "babel-core": "6.14.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "6.5.0",
    "bluebird": "3.4.6",
    "classnames": "2.2.5",
    "jsonfile": "2.4.0",
    "lodash": "4.15.0",
    "react-elm-components": "1.0.0",
    "react-for-atom": "15.3.1-0",
    "rxjs": "5.0.0-beta.12"
  },
  "devDependencies": {
    "standard": "8.1.0"
  },
  "package-deps": [
    "language-elm",
    "elm-format"
  ]
}
